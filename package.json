    "karma": "^0.12.36",
    "karma-jasmine": "^0.3.5",
    "karma-jspm": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.0.0",
    "phantomjs": "^1.9.17"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
